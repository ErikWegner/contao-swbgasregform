{
    "name": "erikwegner/contao-swbgasregform",
    "type": "contao-bundle",
    "description": "SWB Gas Registration Form",
    "homepage": "https://contao.org",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Erik Wegner",
            "homepage": "https://github.com/ErikWegner"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "contao/core-bundle": "^4.9",
        "symfony/config": "^4.4 || ^5.1",
        "symfony/dependency-injection": "^4.4 || ^5.1",
        "symfony/form": "^4.4 || ^5.1",
        "symfony/http-kernel": "^4.4 || ^5.1",
        "erikwegner/fe-openid-provider": "dev-dev"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "contao/easy-coding-standard": "^2.0",
        "phpunit/phpunit": "^8.4",
        "symfony/phpunit-bridge": "^4.4 || ^5.1"
    },
    "extra": {
        "contao-manager-plugin": "Contao\\SwbGasRegForm\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Contao\\SwbGasRegForm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Contao\\SwbGasRegForm\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/ErikWegner/contao-swbgasregform/issues",
        "source": "https://github.com/ErikWegner/contao-swbgasregform"
    },
    "scripts": {
        "cs-fixer": [
            "vendor/bin/ecs check src/ tests/ --fix --ansi"
        ]
    }
}
